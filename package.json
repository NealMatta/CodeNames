{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-grid-gallery": "^0.5.3",
    "react-router": "^3.2.1",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/scss/ -o ./src/css/",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/scss/ -o ./src/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "webpack-cli": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NealMatta/Ryan-Personal-Website.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NealMatta/Ryan-Personal-Website/issues"
  },
  "homepage": "https://github.com/NealMatta/Ryan-Personal-Website#readme"
}
